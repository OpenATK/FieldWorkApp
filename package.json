    "classnames": "^2.2.6",
    "geojson": "^0.5.0",
    "gh-pages": "^2.1.1",
    "leaflet": "^1.6.0",
    "native-base": "^2.12.1",
    "overmind": "^23.0.1",
    "overmind-react": "^24.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.8.6",
    "react-geolocated": "^3.0.1",
